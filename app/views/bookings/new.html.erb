<%= form_for [current_user, @booking] do |f| %>
 <%= f.label :start_date %>
 <%= f.text_field :start_date, placeholder: "mm/dd/yyyy" %>

 <%= f.label :end_date %>
 <%= f.text_field :end_date, placeholder: "mm/dd/yyyy" %>

 <%= f.label :credit_card_number %>
 <%= f.text_field :credit_card_number, placeholder: "Credit Card Number" %>

 <%= f.submit "Submit Request" %>
<% end %>
