<%= form_for [@listing, @booking] do |f| %>
 <%= f.label :start_date %>
 <%= f.text_field :start_date, placeholder: "dd/mm/yyyy" %>

 <%= f.label :end_date %>
 <%= f.date_field :end_date, placeholder: "dd/mm/yyyy" %>

 <%= f.label :credit_card_number %>
 <%= f.date_field :credit_card_number, placeholder: "Credit Card Number" %>

 <%= f.submit "Submit Request" %>
<% end %>
