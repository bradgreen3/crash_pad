<%= form_for @listing do |f| %>
  <%= f.label :city %>
  <%= f.text_field :city, placeholder: "City" %>

  <%= f.label :state %>
  <%= f.text_field :state, placeholder: "State" %>

  <%= f.label :address %>
  <%= f.text_field :address, placeholder: "Address" %>

  <%= f.label :description %>
  <%= f.text_field :description, placeholder: "Description" %>

  <%= f.label :price %>
  <%= f.number_field :price, placeholder: "Price" %>

  <%= f.label :accomodation %>
  <%= f.number_field :accomodation, placeholder: "Accomodation" %>

  <%= f.submit "Create Listing" %>
<% end %>
